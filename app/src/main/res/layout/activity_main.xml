<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="97dp" tools:layout_editor_absoluteX="48dp"
            android:id="@+id/etContraseñaLogin"
            android:hint="Contraseña"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            tools:layout_editor_absoluteY="28dp" tools:layout_editor_absoluteX="48dp" android:id="@+id/etCorreoLogin"
            android:hint="Correo"/>
    <Button
            android:text="ingresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="177dp"
            tools:layout_editor_absoluteX="235dp" android:id="@+id/btnIngresarLogin"/>
    <Button
            android:text="Registrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="262dp"
            tools:layout_editor_absoluteX="238dp" android:id="@+id/btnRegistrarLogin"/>
</android.support.constraint.ConstraintLayout>